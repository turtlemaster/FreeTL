<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
    <definition extends="layout.default" name="page.index">
        <put-attribute name="body" value="/WEB-INF/jsp/index.jsp"/>
    </definition>


    <!-- Transform -->
    <definition extends="layout.default" name="page.transform.list">
        <put-attribute name="body" value="/WEB-INF/jsp/transform/list.jsp"/>
    </definition>

    <definition extends="layout.default" name="page.transform.view">
        <put-attribute name="body" value="/WEB-INF/jsp/transform/view.jsp"/>
        <put-attribute name="extraCss" value="gob" />
        <put-attribute name="extraJs" value="/public/scripts/pages/transform/view.js" />
    </definition>

    <definition extends="layout.default" name="page.transform.new">
        <put-attribute name="body" value="/WEB-INF/jsp/transform/new.jsp"/>

    </definition>

    <definition extends="layout.default" name="page.transform.edit">
        <put-attribute name="body" value="/WEB-INF/jsp/transform/edit.jsp"/>
    </definition>

    <definition extends="layout.default" name="page.transform.edit.confirm">
        <put-attribute name="body" value="/WEB-INF/jsp/transform/confirm.jsp"/>
    </definition>







    <!-- Operations -->

    <definition extends="layout.default" name="page.step.CSVInput">
        <put-attribute name="body" value="/WEB-INF/jsp/step/CSVInput.jsp"/>
        <put-attribute name="extraJs" value="/public/scripts/pages/step/CSVInput.jsp" />
    </definition>

    <definition extends="layout.default" name="page.step.edit">
        <put-attribute name="body" value="/WEB-INF/jsp/step/edit.jsp"/>

    </definition>

    <definition extends="layout.default" name="page.step.confirmEdit">
        <put-attribute name="body" value="/WEB-INF/jsp/step/confirmEdit.jsp"/>

    </definition>


</tiles-definitions>